{
    "name": "johnlitobardinas/fixandfree",
    "type": "project",
    "description": "Automated Subscription Based Application",
    "keywords": [
        "fixandfree",
        "salon",
        "John Lito Bardinas"
    ],
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "bensampo/laravel-enum": "^2.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/sanctum": "^2.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "livewire/livewire": "^1.3",
        "nesbot/carbon": "^2.36",
        "nunomaduro/phpinsights": "^1.14"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "v5.0.0-BETA2",
        "phpunit/phpunit": "~9.2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint:psr12": [
            "./vendor/bin/phpcs --extensions=php --standard=psr12 -n app"
        ],
        "fix:psr12": [
            "./vendor/bin/phpcbf --extensions=php --standard=psr12 -n app"
        ],
        "test:insights": [
            "@php artisan insights"
        ],
        "test:units": [
            "@php artisan test"
        ],
        "test:all":[
            "composer lint:psr12",
            "composer test:insights",
            "composer test:units"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
